{
    "baseUrl": "http://localhost:8080",
    "video": true,
    "viewportWidth": 1680,
    "viewportHeight": 1050,
    "numTestsKeptInMemory": 1,
    "defaultCommandTimeout": 12000,
    "firefoxGcInterval": { "runMode": 1, "openMode": 1 },
    "ignoreTestFiles": ["e2eMasteringflow.spec.tsx", "matching.spec.tsx"],
    "reporter": "junit",
    "reporterOptions": {
      "mochaFile": "results/cypress-report[hash].xml",
      "toConsole": true
    },
    "retries": {
      "runMode": 2,
      "openMode": 0
    },
    "env": {
      "mlHost": "localhost",
      "FAIL_FAST_STRATEGY": "spec"
    }
}
