{
    "baseUrl": "http://localhost:8080",
    "video": true,
    "viewportWidth": 1680,
    "viewportHeight": 1050,
    "numTestsKeptInMemory": 1,
    "defaultCommandTimeout": 10000,
    "reporter": "junit",
    "ignoreTestFiles": "**/addCustomStepToFlow.spec.tsx",
    "reporterOptions": {
      "mochaFile": "results/cypress-report[hash].xml",
      "toConsole": true
    },
    "retries": {
