{
  "name": "e2e",
  "version": "1.0.0",
  "description": "End-to-end Cypress tests.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "cy:run": "npm install && cypress run",
    "cy:run-chrome": "npm install && cypress run --browser chrome --headless",
    "cy:run-firefox": "npm install && cypress run --browser firefox --headless",
    "cy:local": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run-chrome-headed": "npm install && cypress run --browser chrome --headed",
    "cy:run-firefox-headed": "npm install && cypress run --browser firefox --headed"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.5.2",
    "cypress-fail-fast": "^3.4.1"
  }
}
