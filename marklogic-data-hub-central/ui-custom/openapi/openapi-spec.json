      "get": {
        "tags": [
          "Dashboard"
        ],
        "summary": "Returns the entity names for the application.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/entityTypeModels"
                }
              }
            }
          }
        }
      }
    },
    "/api/explore/metrics": {
      "post": {
        "tags": [
          "Dashboard"
        ],
        "summary": "Returns one or more metric values for specified periods of time.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "metrics",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/metricsQuery"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/metrics"
                }
              }
            }
          }
        }
      }
    },
    "/api/explore": {
      "get": {
        "tags": [
          "Search"
        ],
        "summary": "Returns the search results for a query.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "query",
            "in": "query",
            "required": false,
            "schema": {
              "$ref": "#/components/schemas/exploreQuery"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully fetched all the info.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/searchResults"
                }
              }
            }
          },
          "400": {
            "description": "Error while fetching data."
          }
        }
      }
    }
  },
