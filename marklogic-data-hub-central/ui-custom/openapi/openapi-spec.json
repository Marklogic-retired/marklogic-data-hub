  "paths": {
    "/api/explore/login": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "summary": "Performs certificate-based authentication and returns a userid token.",
        "responses": {
          "200": {
            "description": "Successfully received unique id from server."
          },
          "400": {
            "description": "Error while authenticating user."
          }
        }
      },
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Authenticates user with the application server and returns an authorities array.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "metrics",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/credentials"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/authorities"
                }
              }
            }
          }
        }
      }
    },
    "/api/explore/proxyAddress": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "summary": "Returns the login address where certificate-based authentication is performed.",
        "responses": {
          "200": {
            "description": "Successfully received proxy address from server."
          },
          "400": {
            "description": "Error while fetching proxy address."
          }
        }
      }
    },
    "/api/explore/getRecord{recordId}": {
      "get": {
        "tags": [
          "Detail"
        ],
        "summary": "Returns information for a single record based on a record ID.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "recordId",
            "in": "path",
            "description": "ID of record",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/recordDetail"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/api/explore/getRecords": {
      "post": {
        "tags": [
          "Detail"
        ],
        "summary": "Returns information for one or more records based on a list of record IDs.",
        "operationId": "postRecordsUris",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "uris",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/recordUris"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/recordDetail"
                }
              }
            }
          }
        }
      }
    },
    "/api/explore/uiconfig": {
      "get": {
        "tags": [
          "Configuration"
        ],
        "summary": "Returns configuration information for the application UI, including all of the views and widgets.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully received data from server.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/uiconfig"
                }
              }
            }
          },
          "400": {
            "description": "Error while fetching configuration."
          }
        }
      }
    },
    "/api/explore/recentlyVisited": {
      "get": {
        "tags": [
          "Dashboard"
        ],
        "summary": "Returns information for recently visited records.",
        "parameters": [
          {
            "name": "userid",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "userid",
            "in": "query",
            "description": "ID of user",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/recentlyVisited"
                }
              }
            }
          }
        }
      }
