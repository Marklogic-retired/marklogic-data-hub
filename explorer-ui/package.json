    "jest-junit": "^8.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "nock": "^12.0.2",
    "react-app-rewired": "^2.1.3",
    "start-server-and-test": "^1.9.1"
  }
