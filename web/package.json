{
  "name": "data-hub-quickstart",
  "version": "5.0.0",
  "license": "APACHE",
  "homepage": "https://github.com/marklogic/marklogic-data-hub",
  "scripts": {
    "rimraf": "rimraf",
    "kill-connect": "lsof -t -i tcp:8081 -sTCP:LISTEN | xargs kill -9",
    "kill-ng": "lsof -t -i tcp:4200 -sTCP:LISTEN | xargs kill -9",
    "kill-all": "npm run kill-ng && npm run kill-connect",
    "mock:apis": "node ./api/mocks/index",
    "start:uimocks": "ng serve --proxy-config proxy.config-mocks.json --host 0.0.0.0 --disable-host-check",
    "start:ui": "ng serve --proxy-config proxy.config.json --host 0.0.0.0 --disable-host-check",
    "startmocks": "concurrently --kill-others-on-fail \"npm run kill-all\" \"npm run start:uimocks\" \"npm run mock:apis\"",
    "start": "npm run start:ui",
    "clean.dist": "npm run rimraf -- dist",
    "build.prod": "npm run clean.dist && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently --kill-others-on-fail \"npm run kill-ng\" \"npm run kill-connect\" \"./node_modules/.bin/ng e2e\" \"npm run mock:apis\"",
    "webdriver-update": "webdriver-manager update",
    "docs:build": "./node_modules/.bin/compodoc -p ./tsconfig.json -n DHF-QuickStart --theme readthedocs -d site/docs/mlui-angular-docs/ --hideGenerator --disableGraph"
  },
  "private": true,
  "dependencies": {
    "@angular-mdl/core": "^6.0.1",
    "@angular-mdl/popover": "^6.0.0",
    "@angular-mdl/select": "^6.0.0",
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "classlist.js": "^1.1.20150312",
    "clipboard": "^2.0.1",
    "codemirror": "^5.49.2",
    "concurrently": "^4.1.2",
    "core-js": "^2.6.10",
    "date-fns": "^1.29.0",
    "font-awesome": "^4.7.0",
    "http-proxy-middleware": "^0.19.1",
    "js-yaml": "^3.13.1",
