                "collection": [
                  "coll3",
                  "coll4"
                ]
              }
            },
            "onArchive": {
              "remove": {
                "collection": [
                  "arch-coll"
                ]
              }
            }
          }
        },
        "mergeStrategies": [
          {
            "name": "CRM Source",
            "algorithmRef": "standard",
            "sourceWeights": [
              {
                "source": {
                  "name": "CRM",
                  "weight": 3
                }
              },
              {
                "source": {
                  "name": "ERP",
                  "weight": 1
                }
              }
            ]
          },
          {
            "name": "Length-Weight",
            "algorithmRef": "standard",
            "length": {
              "weight": 12
            }
          }
        ],
        "merging": [
          {
            "propertyName": "ssn",
            "maxValues": 1,
            "length": {
              "weight": 5
            }
          },
          {
            "propertyName": "firstName",
