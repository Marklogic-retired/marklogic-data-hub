            }
          },
          "custom": [
            {
              "name": "customMerge",
              "at": "/smart-mastering/merge/customMerge.sjs",
              "namespace": "",
              "function": "customMerge"
            }
          ],
          "collections": {
            "onMerge": {
              "remove": {
                "collection": [
                  "coll-to-add"
