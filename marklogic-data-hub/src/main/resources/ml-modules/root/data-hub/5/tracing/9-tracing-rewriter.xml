    <!-- XDBC -->

  <!-- tracing rewrite entries -->
  <match-path matches="^/?$">
    <dispatch>/trace-ui/index.html</dispatch>
  </match-path>

  <match-path matches="^/?$|^/content/([^/]+)/?$">
    <dispatch>/trace-ui/index.html</dispatch>
  </match-path>

  <match-path matches="^/.*\.(ico|js|css|ttf|eot|woff|woff2|svg)$">
