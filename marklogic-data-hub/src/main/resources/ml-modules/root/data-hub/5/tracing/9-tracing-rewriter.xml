    <add-query-param name="extension">$1</add-query-param>
    <dispatch>/data-hub/4/tracing/trace-ui.xqy</dispatch>
  </match-path>

  <match-path matches="^/assets/.+$">
    <add-query-param name="uri">$0</add-query-param>
    <dispatch>/data-hub/4/tracing/trace-ui.xqy</dispatch>
  </match-path>
  <!-- tracing rewrite entries -->

