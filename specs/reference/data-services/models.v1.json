{
  "openapi": "3.0.0",
  "info": {
    "title": "models",
    "version": "1.0",
    "description": "Data services for endpoints pertaining to entity models"
  },
  "servers": [
    {
      "url": "http://localhost:8011/data-hub/5/data-services/models"
    }
  ],
  "paths": {
    "/getPrimaryEntityTypes.sjs": {
      "post": {
        "summary": "getPrimaryEntityTypes",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "../../models/PrimaryEntityTypes.v1.json"
                }
              }
            }
          }
        },
        "operationId": "getPrimaryEntityTypes.sjs",
        "description": "Returns an array of primary entity types. A primary entity type is the entity type in a model descriptor with a name equal to the title of the model descriptor."
      },
      "parameters": []
    },
    "/createModel.sjs": {
      "post": {
        "summary": "createModel",
        "operationId": "post-createModelsjs",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "../../models/ModelDescriptor.v1.json"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "required": [
                  "name"
                ]
              }
            }
          }
        },
        "description": "Create a new model, resulting in a new entity descriptor with a primary entity type in it."
      },
      "parameters": []
    },
    "/updateModelInfo.sjs": {
      "post": {
        "summary": "Update model info",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "operationId": "get-updateModelInfo.sjs",
        "description": "Update the description of an existing model. Model title and version cannot yet be edited because doing so would break existing mapping and mastering configurations.",
        "requestBody": {},
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "name",
            "description": "The name of the model"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "description"
          }
        ]
      },
      "parameters": []
    },
    "/updateModelEntityTypes.sjs": {
      "post": {
        "summary": "Update model entity types",
        "operationId": "post-updateModelEntityTypes.sjs",
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "name",
            "description": "The name of the model"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../../models/ModelDefinitions.v1.json"
              }
            }
          }
        }
      }
    }
  }
}