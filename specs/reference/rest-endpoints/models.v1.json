{
  "openapi": "3.0.0",
  "info": {
    "title": "models",
    "version": "1.0",
    "description": "Endpoints for operations pertaining to entity models"
  },
  "servers": [
    {}
  ],
  "paths": {
    "/api/models/primaryEntityTypes": {
      "get": {
        "summary": "Get primary entity types",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "../../models/PrimaryEntityTypes.v1.json"
                }
              }
            }
          }
        },
        "operationId": "get-api-entities",
        "description": "Get all primary entity types, where an entity type is considered \"primary\" if its name is the same as the info/title in the model descriptor that it belongs to. A model descriptor thus can only have one primary entity type."
      },
      "parameters": []
    },
    "/api/models/${name}/info": {
      "post": {
        "summary": "Edit model info",
        "operationId": "post-api-models-info",
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "../../models/ModelDescriptor.v1.json"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "../../models/RESTError.v1.json"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "description": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "parameters": [],
        "description": "Edit the info of a model. Name and baseUri are not yet editable, as changing those will cause mapping and mastering configurations to break. "
      },
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "name",
          "in": "path",
          "required": true,
          "description": "The name of the entity model"
        }
      ]
    },
    "/api/models/${name}/entityTypes": {
      "put": {
        "summary": "Update entity type definitions",
        "tags": [],
        "responses": {
          "200": {
            "description": "No response body is returned, as no additional information is generated by the server as a result of updating the set of entity types."
          }
        },
        "operationId": "put-api-entityTypes",
        "description": "Update the set of entity types in the model identified by modelId.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../../models/ModelDefinitions.v1.json"
              }
            }
          },
          "description": "The request body is expected to be the entire set of entity types (\"definitions\"), which will then overwrite the existing definitions in the model identified by name. "
        }
      },
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "name",
          "in": "path",
          "required": true,
          "description": "The name of the entity model"
        }
      ]
    },
    "/api/models": {
      "post": {
        "summary": "Create new model",
        "operationId": "post-api-models",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "model": {
                      "$ref": "../../models/ModelDescriptor.v1.json"
                    }
                  }
                }
              }
            }
          }
        },
        "description": "Create a new model, which results in a new model descriptor with a primary entity type in it with the same name as the name of the model. The model name and entire model descriptor are returned.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "required": [
                  "name"
                ]
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
  }
}